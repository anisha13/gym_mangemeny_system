@model Gym.Models.Entity.AdminEntity
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Layout/_mainlayout.cshtml";
}

<div class="card-box">

	<h3 style="color:#008080; margin-bottom:0px;"><i class="md md-details"></i> Detail Admin</h3>
	<table>
		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.admin_id)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.admin_id)
			</td>
		</tr>
		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.admin_fullname)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.admin_fullname)
			</td>
		</tr>
		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.admin_username)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.admin_username)
			</td>
		</tr>
		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.admin_password)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.admin_password)
			</td>
		</tr>

	</table>
	</div>
<p>
	<i class="md md-arrow-back"></i> @Html.ActionLink("Back to Manager", "Index", "Admin", null, new { @class = "menu" })
</p>