@model Gym.Models.Entity.PlanEntity
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Layout/_mainlayout.cshtml";
}


<div class="card-box">
	<table>
		<h3 style="color:#008080; margin-bottom:0px;">Detail plan</h3>

		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.plan_id)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.plan_id)
			</td>
		</tr>
		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.plan_name)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.plan_name)
			</td>
		</tr>
		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.plan_cost)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.plan_cost)
			</td>
		<tr>
			<td width="100px">
				@Html.LabelFor(m => m.plan_period)
			</td>
			<td width="200px">
				@Html.DisplayFor(m => m.plan_period)
			</td>

		</tr>

	</table>
	</div>
<p>
	<i class="md md-arrow-back"></i> @Html.ActionLink("Back to Manager", "Index", "Plan", null, new { @class = "menu" })
</p>