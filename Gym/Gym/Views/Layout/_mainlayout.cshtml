			<script src="/Content/assets/pages/jquery.todo.js"></script>

			<!-- chatjs  -->
			<script src="/Content/assets/pages/jquery.chat.js"></script>

			<script src="/Content/assets/plugins/peity/jquery.peity.min.js"></script>

